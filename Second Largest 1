Objective:
WAP to find second largest among three numbers.


#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    char comma;
   
    cin >> a >> comma >> b >> comma >> c;
   
    int arr[3] = {a, b, c};
    sort(arr, arr + 3);
   
    cout << arr[1] << endl;
   
    return 0;
}
